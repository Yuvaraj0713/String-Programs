package String;
public class RemoveVowels {
    public static void main(String[] args) {
        String s = "education";
        String result = "";
        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i);
            if (!(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')) {
                result = result + ch;
            }
        }
        System.out.println(result);
    }
}
